public class Student {
   int studentAge;

 public Student(String name) {
      // This constructor has one parameter, name.
      System.out.println("Student's Full Name :" + name );
   }

   public void setAge( int age ) {
      studentAge = age;
   }

   public int getAge( ) {
      System.out.println("Student's Age :" + studentAge );
      return studentAge;
   }

   public static void main(String []args) {
      /* Object creation */
      Student myStudent = new Student( "NAJLA DANIYA KHANSA" );
     
      /* Call class method to set Student's age */
      myStudent.setAge(18 );
    myStudent.getAge( );

   }
}
